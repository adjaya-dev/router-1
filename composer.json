{
    "name": "atanvarno/router",
    "description": "FastRoute PSR-7 wrapper",
    "keywords": [
        "PSR-7", "router", "FastRoute"
    ],
    "homepage": "https://github.com/atanvarno69/dependency",
    "license": "MIT",
    "authors": [
        {
            "name": "atanvarno69",
            "email": "atanvarno69@gmail.com",
            "homepage": "https://github.com/atanvarno69",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "nikic/fast-route": "^1.2.0",
        "psr/http-message": "^1.0.1",
        "psr/simple-cache": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8"
    },
    "autoload": {
        "psr-4": {
            "Atanvarno\\Router\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atanvarno\\Router\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
